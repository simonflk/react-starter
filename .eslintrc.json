{
    "env": {
        "browser": true,
        "es2020": true,
        "jest": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:import/errors",
        "prettier"
    ],
    "settings": {
        "react": {
            "version": "16.12.0"
        }
    },
    "plugins": [
        "react",
        "react-hooks",
        "import"
    ],
    "globals": {
        "__VERSION__": "readonly",
        "__COMMITHASH__": "readonly",
        "__BRANCH__": "readonly",
        "__DEV__": "readonly",
        "__DEBUG__": "readonly"
    },
    "rules": {
        "no-console": ["warn", {"allow": ["warn", "error"]}]
    }
}
